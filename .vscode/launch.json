{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
      {
          "name": "(gdb) Launch",
          "type": "cppdbg",
          "request": "launch",
          "program": "${workspaceFolder}/build/msp430-softmmu/qemu-system-msp430",
          "args": [
            "-machine",
            "b_msp430fr5739",
            "-mcu",
            "msp430fr5739_mcu",
            "-kernel",
            "/mnt/d/workspaces/IAR/msp430_flash_gpio/430/examples/Flashing the LED/Flashing the LED/msp430x5xx/C-source/Debug/Exe/msp430x5xx (C).a43",
            "-nographic"
          ],
          "stopAtEntry": false,
          "cwd": "${workspaceFolder}",
          "environment": [],
          "externalConsole": false,
          "MIMode": "gdb",
          "setupCommands": [
              {
                  "description": "Enable pretty-printing for gdb",
                  "text": "-enable-pretty-printing",
                  "ignoreFailures": true
              }
          ]
      },
      {
        "name": "Python: Current File (Integrated Terminal)",
        "type": "python",
        "request": "launch",
        "program": "${file}",
        "console": "integratedTerminal",
        "cwd": "${workspaceFolder}/hw/msp430/mcus_db/tools",
      }
  ]
}